FROM debian:jessie

RUN apt-get update && apt-get install -y bash-completion build-essential curl ca-certificates debhelper devscripts dh-systemd git libapparmor-dev libseccomp-dev libwww-perl --no-install-recommends && rm -rf /var/lib/apt/lists/*

ENV GO_VERSION 1.4.2
RUN curl -fSL "https://storage.googleapis.com/golang/go${GO_VERSION}.linux-amd64.tar.gz" | tar xzC /usr/local
ENV PATH $PATH:/usr/local/go/bin
ENV GOPATH /go
